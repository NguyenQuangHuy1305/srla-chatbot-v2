{
  "routes": [
    {
      "route": "/api/chat",
      "methods": [
        "post",
        "get",
        "head",
        "put",
        "delete",
        "connect",
        "options",
        "trace",
        "patch"
      ]
    },
    {
      "route": "/*",
      "serve": "/frontend/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}